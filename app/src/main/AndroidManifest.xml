<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Project.minimaltodo">

    <uses-permission android:name="ANDROID.PERMISSION.INTERNET" />
    <uses-permission android:name="ANDROID.PERMISSION.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.example.danny.chick.Analytics.AnalyticsApplication"
        android:allowBackup="true"
        android:icon="@drawable/chicksprof"
        android:label="@string/app_name"
        android:theme="@style/CustomStyle.DarkTheme">
        <activity android:name="com.example.danny.chick.MainActivityPoultry"></activity>
        <activity android:name="com.example.danny.chick.symptoms"></activity>
        <activity android:name="com.example.danny.chick.Search"></activity>
        <activity android:name="com.example.danny.chick.Register"></activity>
        <activity android:name="com.example.danny.chick.InitialCheck"></activity>
        <activity android:name="com.example.danny.chick.Homepage"></activity>
        <activity android:name="com.example.danny.chick.diseaseref"></activity>
        <activity android:name="com.example.danny.chick.Login"></activity>
        <activity android:name="com.example.danny.chick.Main.MainActivity"></activity>
        <activity
            android:name="com.example.danny.chick.welcome"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.danny.chick.AddToDo.AddToDoActivity"
            android:parentActivityName="com.example.danny.chick.Main.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.danny.chick.About.AboutActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.example.danny.chick.Main.MainActivity" />
        <activity
            android:name="com.example.danny.chick.Reminder.ReminderActivity"
            android:label="@string/app_name" />

        <service android:name="com.example.danny.chick.Utility.TodoNotificationService" />
        <service android:name="com.example.danny.chick.Utility.DeleteNotificationService" />

        <activity
            android:name="com.example.danny.chick.Settings.SettingsActivity"
            android:parentActivityName="com.example.danny.chick.Main.MainActivity" />
    </application>

</manifest>